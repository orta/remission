{
  "files.exclude": {
    ".git/": true,
    ".awcache/": true,
    "node_modules/": false,
    "dist/": false,
    "externals/": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "dist/": true
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "prettier.singleQuote": false,
  "prettier.trailingComma": "es5",
  "prettier.typescriptEnable": ["typescript", "typescriptreact"],
  "prettier.semi": false,
  "prettier.printWidth": 120,
  "typescript.tsdk": "./node_modules/typescript/lib"
}
